{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": "> 0.5%, ie >= 11"
                },
                "modules": false,
                "spec": true,
                "useBuiltIns": "usage",
                "forceAllTransforms": true,
                "corejs": {
                    "version": 3,
                    "proposals": false
                }
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ]
}