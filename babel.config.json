{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": "> 0.5%"
                },
                "modules": false,
                "spec": true,
                "useBuiltIns": "usage",
                "forceAllTransforms": true,
                "corejs": {
                    "version": "2", 
                    "proposals": true
                }
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/proposal-object-rest-spread"
    ]
}